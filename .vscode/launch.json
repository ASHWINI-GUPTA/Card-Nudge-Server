    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "name": "Launch Summary Program",
            "type": "node",
            "program": "${workspaceFolder}/functions/generate-card-summaries/index.ts",
            "cwd": "${workspaceFolder}",
            "env": {},
            "runtimeExecutable": "/Volumes/External/Application/homebrew/bin/deno",
            "runtimeArgs": [
                "run",
                "--inspect-brk",
                "--allow-env",
                "--allow-read",
                "--allow-net",
                "--env-file=.env"
            ],
            "attachSimplePort": 9229
        }
    ]
}